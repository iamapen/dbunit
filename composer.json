{
    "name": "iamapen/dbunit",
    "description": "PHPUnit extension for database interaction testing",
    "type": "library",
    "keywords": [
        "database",
        "testing",
        "xunit"
    ],
    "homepage": "https://github.com/iamapen/dbunit/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "iamapen",
            "email": "iamapen@studiopoppy.com"
        },
        {
            "name": "Sebastian Bergmann",
            "email": "sebastian@phpunit.de",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/iamapen/dbunit/issues"
    },
    "require": {
        "php": "^7.2",
        "phpunit/phpunit": "^8.0",
        "symfony/yaml": "^3.0 || ^4.0",
        "ext-pdo": "*",
        "ext-simplexml": "*"
    },
    "replace": {
        "phpunit/dbunit": "5.*"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        }
    }
}
